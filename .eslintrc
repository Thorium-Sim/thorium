{
  "extends": ["eslint:recommended", "react-app", "eslint-config-prettier"],
  "rules": {
    "no-console": "off",
    "no-inner-declarations": 0,
    "no-throw-literal": 0
  },
  "ignorePatterns": [
    "/node_modules/",
    "/client/coverage/",
    "/build/",
    "/server/build/",
    "/.cache/",
    "/client/locales/",
    "/client/src/models/"
  ]
}
