{
  "name": "thorium",
  "version": "3.13.1",
  "description": "Starship Simulator Controls",
  "private": true,
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/Thorium-Sim/thorium.git"
  },
  "author": "Alex Anderson <alexanderson1993@gmail.com>",
  "bugs": {
    "url": "https://github.com/Thorium-Sim/thorium/issues"
  },
  "license": "Apache-2.0",
  "main": "./build/electron.js",
  "homepage": "/",
  "languages": [
    "es",
    "fr",
    "it",
    "nl",
    "de"
  ],
  "scripts": {
    "start": "npm-run-all --parallel start:client start:server",
    "start:client": "vite",
    "start:server": "ts-node-dev --project server/tsconfig.json server/index.ts",
    "watch:electron": "wait-on http://localhost:3000 && electron .",
    "start:electron": "BROWSER=none npm-run-all --parallel -l start:client start:server watch:electron",
    "build": "cross-env NODE_ENV=production npm-run-all build:*",
    "build:client": "vite build",
    "build:server": "tsc -p server",
    "build:ncc": "ncc build -o build/server -s tempServer/server/index.js",
    "build:env": "node ./scripts/build-env.js",
    "build:copy": "cp package.json build/package.json",
    "generate:schema": "get-graphql-schema http://localhost:3001/graphql > src/schema.graphql",
    "generate:electron": "electron-builder -mwl",
    "generate:apollo": "apollo service:push",
    "dist:electron": "electron-builder -mwl -p always",
    "pack:electron": "electron-builder -m",
    "generate:pkg": "NODE_ENV=production pkg build/package.json",
    "test": "is-ci test:plain test:watch",
    "test:watch": "vitest watch",
    "test:plain": "vitest run",
    "translate": "babel-node scripts/manageTranslations.js",
    "plop": "plop",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "eject": "react-app-rewired eject",
    "lint": "eslint .",
    "typecheck": "npm-run-all --parallel typecheck:client typecheck:server",
    "typecheck:client": "tsc --noEmit",
    "typecheck:server": "tsc --noEmit -p server",
    "stylelint": "stylelint ./src",
    "bump": "node ./scripts/bumpVersion.js",
    "generate:graphql": "graphql-codegen --config codegen.yml",
    "format": "prettier --write \"**/*.+(js|jsx|yml|yaml|css|less|scss|ts|tsx|md|mdx|graphql|vue)\"",
    "format:different": "prettier --list-different \"**/*.+(js|jsx|yml|yaml|css|less|scss|ts|tsx|md|mdx|graphql|vue)\"",
    "validate": "CI=true npm-run-all --parallel test:plain typecheck format:different",
    "lint-staged-validate": "lint-staged",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "proxy": "http://localhost:3001",
  "nodemonConfig": {
    "watch": [
      "server/"
    ],
    "ignore": [
      ".git",
      "./src/*",
      "./public/*",
      "./.config/*",
      "./types/*"
    ]
  },
  "browserslist": [
    "maintained node versions",
    "last 1 chrome version",
    "last 1 firefox version",
    "last 1 safari version"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ],
    "*.{css,scss}": [
      "stylelint"
    ],
    "**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|mdx|graphql|vue)": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "build": {
    "appId": "us.fyreworks.thorium",
    "productName": "Thorium",
    "buildDependenciesFromSource": true,
    "nodeGypRebuild": false,
    "npmRebuild": true,
    "asar": false,
    "directories": {
      "output": "./packages"
    },
    "publish": {
      "provider": "github"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "!node_modules/.bin",
      "!node_modules/.cache",
      "!node_modules/.git",
      "!node_modules/.github",
      "!node_modules/.husky",
      "!node_modules/.idea",
      "!node_modules/.vscode",
      "!node_modules/.yarn-integrity",
      "!node_modules/.yarn-unplugged",
      "!node_modules/.yarnrc",
      "!node_modules/.yarnrc.yml",
      "!node_modules/.yarnignore",
      "!node_modules/.npmignore",
      "!node_modules/.npmrc",
      "!node_modules/.nvmrc",
      "!node_modules/.eslintrc",
      "!**/.vscode/*",
      "!src/*",
      "!server/*",
      "!electron.vite.config.{js,ts,mjs,cjs}",
      "!{.eslintignore,.eslintrc.cjs,.prettierignore,.prettierrc.yaml,dev-app-update.yml,CHANGELOG.md,README.md}",
      "'!{.env,.env.*,.npmrc,pnpm-lock.yaml}",
      "!{tsconfig.json,tsconfig.node.json,tsconfig.web.json}"
    ],
    "mac": {
      "category": "public.app-category.entertainment",
      "icon": "./public/icon.icns",
      "hardenedRuntime": true
    },
    "win": {
      "target": [
        "zip",
        "portable"
      ],
      "icon": "./public/icon.ico"
    },
    "linux": {
      "category": "Utility"
    }
  },
  "bin": "./server/index.js",
  "pkg": {
    "assets": [
      "index.html",
      "css/**/*",
      "fonts/**/*",
      "js/**/*",
      "static/**/*",
      "cardIcons/**/*",
      "workers/**/*",
      "systems/**/*",
      "viewscreen/**/*",
      "sciences.ogg",
      "favicon.ico",
      ".env"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "tagFormat": "${version}",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "npm run bump ${nextRelease.version}",
          "publishCmd": "npm run build"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "babelMacros": {
    "styledComponents": {}
  },
