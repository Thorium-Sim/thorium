    "@swc/core": "^1.0.39",
    "babel-jest": "^24.1.0",
    "babel-plugin-import-directory": "^1.0.3",
    "babel-plugin-import-glob": "^2.0.0",
    "babel-plugin-wildcard-hayneedle": "^4.0.2",
    "eslint-plugin-react-hooks": "^1.3.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "regenerator-runtime": "^0.13.1"
  }
