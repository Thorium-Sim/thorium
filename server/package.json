{
  "name": "thorium-server",
  "version": "1.2.17",
  "description": "Starship Simulator Controls Server, built with GraphQL",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Thorium-Sim/thorium.git"
  },
  "author": "Alex Anderson <alexanderson1993@gmail.com>",
  "bugs": {
    "url": "https://github.com/Thorium-Sim/thorium/issues"
  },
  "license": "Apache-2.0",
  "homepage": "https://thoriumsim.com",
  "scripts": {
    "prestart": "rm -f ~/.babel.json",
    "start": "nodemon ./src/server.js -e js,graphql --ignore snapshots/ --exec babel-node",
    "build": "npm-run-all clean-build build-js",
    "build-js": "NODE_ENV=production babel src -d build",
    "clean": "rm -rf node_modules",
    "clean-build": "rm -rf build",
    "test": "jest --setupFiles ./tests/setup.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@sentry/node": "^4.6.4",
    "apollo-server": "^2.4.8",
    "apollo-server-express": "^2.4.8",
    "appdirectory": "^0.1.0",
    "body-parser": "^1.18.3",
    "bonjour": "^3.5.0",
    "chalk": "^2.4.2",
    "change-case": "^3.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-status-monitor": "^1.2.3",
    "graphql": "^14.1.1",
    "graphql-server-express": "^1.4.0",
    "graphql-subscriptions": "^1.0.0",
    "graphql-tools": "^4.0.4",
    "graphql-type-json": "^0.2.1",
    "graphqurl": "^0.3.2",
    "jest": "^24.1.0",
    "luxon": "^1.11.3",
    "multer": "^1.4.1",
    "natives": "^1.1.6",
    "ncp": "^2.0.0",
    "node-fetch": "^2.3.0",
    "random-words": "^1.1.0",
    "react-dev-utils": "^7.0.3",
    "semver": "^5.6.0",
    "simplex-noise": "^2.4.0",
    "subscriptions-transport-ws": "^0.9.15",
    "three": "^0.101.1",
    "unzip": "^0.1.11",
    "uuid": "^3.3.2",
    "yauzl": "^2.10.0",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-stage-3": "^7.0.0",
